      "id": "list-workflows",
      "name": "Listar Workflows",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [900, 300],
      "credentials": {
        "n8nApi": {
          "id": "n8n-api-credentials",
          "name": "n8n API"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "// Filtrar workflows do agente específico\nconst workflows = $input.first().json;\nconst agentName = $('Normalizar Dados').item.json.agentName;\nconst agentId = $('Normalizar Dados').item.json.agentId;\n\nconsole.log('🔍 Listando workflows:', workflows.length);\nconsole.log('🎯 Procurando por agente:', agentName);\n\n// Filtrar workflows que pertencem a este agente\nconst agentWorkflows = workflows.filter(workflow => {\n  const name = workflow.name || '';\n  return name.includes(agentName) || name.includes(agentId);\n});\n\nconsole.log('✅ Workflows encontrados para deletar:', agentWorkflows.length);\nagentWorkflows.forEach(w => console.log('  -', w.name, '(ID:', w.id, ')'));\n\nreturn agentWorkflows.map(workflow => ({\n  json: {\n    id: workflow.id,\n    name: workflow.name,\n    active: workflow.active\n  }\n}));"
      },
      "id": "filter-workflows",
      "name": "Filtrar Workflows do Agente",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1120, 300]
    },
    {
      "parameters": {
        "method": "DELETE",
        "url": "=https://n8n.code-iq.com.br/api/v1/workflows/{{ $json.id }}",
        "authentication": "predefinedCredentialType",
        "nodeCredentialType": "n8nApi",
        "options": {}
      },
      "id": "delete-workflow",
      "name": "Deletar Workflow",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [1340, 300],
      "credentials": {
        "n8nApi": {
          "id": "n8n-api-credentials",
          "name": "n8n API"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "// Consolidar resultados da deleção\nconst deletedFromDb = $('Deletar do Banco').item.json;\nconst deletedWorkflows = $input.all();\n\nconst result = {\n  success: true,\n  message: 'Agente e workflows deletados com sucesso',\n  agent: {\n    id: deletedFromDb.id,\n    name: deletedFromDb.nome,\n    workflow_id: deletedFromDb.workflow_id,\n    webhook_url: deletedFromDb.webhook_url\n  },\n  deleted_workflows: deletedWorkflows.map(item => ({\n    id: item.json.id,\n    name: item.json.name,\n    deleted: true\n  })),\n  summary: {\n    total_workflows_deleted: deletedWorkflows.length,\n    timestamp: new Date().toISOString()\n  }\n};\n\nconsole.log('✅ Deleção completa:', result);\n\nreturn { json: result };"
      },
      "id": "consolidate-results",
      "name": "Consolidar Resultados",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1560, 300]
