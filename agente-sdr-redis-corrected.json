    {
      "parameters": {
        "jsCode": "const lead = $json.lead_data;\n\n// SaudaÃ§Ã£o por horÃ¡rio (SP)\nfunction saudacaoSP() {\n  const horas = parseInt(new Intl.DateTimeFormat('pt-BR', {\n    timeZone: 'America/Sao_Paulo', hour: 'numeric', hour12: false\n  }).format(new Date()), 10);\n  if (horas >= 5 && horas < 12) return 'Bom dia';\n  if (horas >= 12 && horas < 19) return 'Boa tarde';\n  return 'Boa noite';\n}\n\nconst saudacao = saudacaoSP();\nconst nome = lead.nome_cliente || lead.nome || 'Cliente';\nconst profissao = lead.profissao || 'sua Ã¡rea de atuaÃ§Ã£o';\nconst fonte = lead.fonte_prospec || 'um parceiro';\n\nconst mensagem = `${saudacao}, ${nome}! Sou Cleber Frank da Elleve ConsÃ³rcios. Recebi seu contato atravÃ©s de ${fonte} e gostaria de falar sobre oportunidades de investimento. Como atua com ${profissao}, acredito que pode se interessar por soluÃ§Ãµes financeiras sÃ³lidas. Posso te explicar rapidamente como funciona, o que acha?`;\n\nconsole.log(`ðŸ’¬ Mensagem gerada para ${nome}: ${mensagem.substring(0, 50)}...`);\n\nreturn [{\n  json: {\n    ...$json,\n    mensagem,\n    telefone_normalizado: lead.telefone?.toString().replace(/\\D/g, '') || '',\n    nome_cliente: nome,\n    profissao_cliente: profissao\n  }\n}];"
      },
